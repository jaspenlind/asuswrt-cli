{
  "bin": {
    "router": "dist/src/bin/router.mjs"
  },
  "description": "ASUS Router CLI",
  "files": [
    "dist/src/*"
  ],
  "version": "0.1.0",
  "main": "src/lib/bin/router.cjs",
  "name": "asuswrt-cli",
  "types": "dist/src/types/index.d.ts",
  "repository": "github:jaspenlind/asuswrt-cli",
  "readme": "https://github.com/jaspenlind/asuswrt-cli#readme",
  "author": "jaspenlind",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "firewall",
    "vpn",
    "asuswrt",
    "cli",
    "tools"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./lib_esm/index.mjs"
      },
      "require": "./lib_cjs/index.cjs"
    }
  },
  "scripts": {
    "build": "npx tsc",
    "start": "ts-node src/lib/bin/router.mts",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --coverage -t",
    "clean": "rimraf coverage dist",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "debug": "node --nolazy --inspect-brk=9229 node_modules/jest/bin/jest.js -i",
    "format": "prettier --ignore-path .eslintignore --write \"**/*.{js,ts,jsx,tsx}\" -c",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "postversion": "git push && git push --tags && npm run clean",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && num run lint",
    "release:alpha": "npm version prerelease --preid alpha -m \" build: :bookmark:%s\"",
    "release:beta": "npm version prerelease --preid beta -m \"build: :bookmark: %s\"",
    "release:patch": "npm version patch -m \"build: :bookmark: %s\"",
    "release:minor": "npm version minor -m \"build: :bookmark: %s\"",
    "release:major": "npm version major -m \"build: :bookmark: %s\"",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules npx jest --watch-all -t",
    "version": "git add -A src"
  },
  "dependencies": {
    "args-any": "^1.4.3",
    "chalk": "^5.0.1",
    "cron-parser": "^4.5.0",
    "dotenv": "^16.0.1",
    "flexi-path": "1.5.2",
    "nordvpn-server-lister": "^0.6.2",
    "promptly": "^3.2.0",
    "shelljs": "^0.8.5",
    "ssh-keygen-lite": "^1.3.0",
    "table": "^6.8.0",
    "ts-node": "^10.8.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/jest": "^28.1.4",
    "@types/promptly": "^3.0.2",
    "@types/shelljs": "^0.8.11",
    "@types/node": "^18.0.3",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "commitlint": "^17.0.3",
    "coveralls": "^3.1.1",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.5.0",
    "expect": "^28.1.1",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "jest-extended": "^2.0.0",
    "jest-matcher-utils": "^28.1.1",
    "lint-staged": "^13.0.3",
    "minimist": "^1.2.6",
    "prettier": "^2.7.1",
    "set-value": "^4.1.0"
  }
}
